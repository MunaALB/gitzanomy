        <script>
            function searchProductData(o){
                var valueData=$.trim($("#search_product_text").val());
                if(valueData){
                    window.location.href="<?=base_url('search-product/');?>"+encodeURI(valueData);
                }
            }
            
            function submissionSearchForm(o){
                if (event.keyCode === 13) {
                        event.preventDefault();
                        $("#searchProductButton").click();
                    }
            }
            function searchProductDataKey(o){
                var getData=$.trim($(o).val());
                $("#searchResultDiv").css('display','block');
                if(getData){
                    $("#searchResultSpinner").css('display','block');
                    var reg_form_data = new FormData();
                    reg_form_data.append("user_id","00");
                    reg_form_data.append("search",getData);
                    reg_form_data.append("limit","100");
                    reg_form_data.append("start","0");
                    $.ajax({
                        url: baseUrl+'searchProductWithCategory',
                        type: "POST",
                        data: reg_form_data,
                        enctype: 'multipart/form-data',
                        cache: false,
                        contentType: false,
                        processData: false,
                        success: function (data) {
                            //returnObject = JSON.parse(JSON.stringify(data));
                            returnObject = JSON.parse((data));
                            // console.log('asasasas' , returnObject['data']['category']);
                            var categoryData=returnObject['data']['category'];
                            var brandData=returnObject['data']['brand'];
                            var productData=returnObject['data']['product'];
                            console.log('categoryData',categoryData)
                            if(categoryData.length>0){
                                var htmldata='';
                                $(categoryData).each(function( index, value ) {
                                    console.log('value',value['name'])
                                    htmldata+='<li><a href="<?=base_url();?>product-subcategory/'+value['category_id']+'/'+encodeURIComponent(value['name'])+'"><img src="'+value['image']+'"><p>'+value['name']+'</p></a></li>';
                                });
                                $("#searchCaregoryUl").empty();
                                $("#searchCaregoryUl").append(htmldata);
                                $("#searchCaregoryDiv").css('display','block');
                            }else{
                                $("#searchCaregoryDiv").css('display','none');
                            }
                            if(brandData.length>0){
                                var htmldata='';
                                $(brandData).each(function( index, value ) {
                                    console.log('value',value['name'])
                                    htmldata+='<li><a href="<?=base_url();?>product-by-brand/'+value['brand_id']+'/'+encodeURIComponent(value['name'])+'"><img src="'+value['image']+'"><p>'+value['name']+'</p></a></li>';
                                });
                                $("#searchBrandUl").empty();
                                $("#searchBrandUl").append(htmldata);
                                $("#searchBrandDiv").css('display','block');
                            }else{
                                $("#searchBrandDiv").css('display','none');
                            }
                            if(productData.length>0){
                                var htmldata='';
                                $(productData).each(function( index, value ) {
                                    console.log('value',value['name'])
                                    htmldata+='<li><a href="<?=base_url();?>product-detail/'+encodeURIComponent(value['name'])+'/'+value['product_id']+'"><img src="'+value['images'][0]['image']+'"><p>'+value['name']+'</p></a></li>';
                                });
                                $("#searchProductUl").empty();
                                $("#searchProductUl").append(htmldata);
                                $("#searchProductDiv").css('display','block');
                            }else{
                                $("#searchProductDiv").css('display','none');
                            }

                            $("#searchResultSpinner").css('display','none');
                        },
                        error: function (error) {
                            alert("error");
                        }
                    });
                }
            }

            function closeSearchDiv(o,i,d){
                $("#"+d).empty();
                $("#"+i).css('display','none');
            }
        </script>
        <script>
            function changeLanguage(o,i){
                $.ajax({
                    url: "<?php echo base_url("web/Web/ajax") ?>",
                    type: "POST",
                    data: "method=changeLanguage&i=" + i,
                    // data: "method=changeLanguage&i=en",
                    success: function (data) {
                        var dta = $.trim(data);
                        var jsonData = $.parseJSON(dta);
                        location.reload();
                    }
                })
            }
        </script>
